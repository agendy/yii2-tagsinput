{
  "name": "efureev/yii2-tagsinput",
  "description": "Bootstrap Tags Input is a jQuery plugin providing a Twitter Bootstrap user interface for managing tags.",
  "version": "0.1.1",
  "keywords": [
    "yii2",
    "tags",
    "tag",
    "input",
    "typeahead"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "minimum-stability": "stable",
  "support": {
    "issues": "https://github.com/efureev/yii2-tagsinput/issues",
    "source": "https://github.com/efureev/yii2-tagsinput"
  },
  "authors": [
    {
      "name": "Feugene",
      "email": "fureev@gmail.com",
      "homepage": "https://github.com/efureev/yii2-tagsinput"
    }
  ],
  "require": {
    "bower-asset/fe-tagsinput": "~0.8",
    "bower-asset/typeahead.js": "dev-master",
    "yiisoft/yii2": "~2"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.5",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "efureev\\tagsinput\\": "src/"
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
